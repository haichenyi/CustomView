<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="haichenyi.aloe.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.WaveViewActivity" />
        <activity android:name=".activity.CircleProgressActivity" />
        <activity android:name=".activity.VerifyButtonActivity" />
        <activity android:name=".activity.VerifyCodeViewActivity" />
        <activity android:name=".activity.NumberPickActivity" />
    </application>

</manifest>